/*
    Brewer color palettes, for diverging, qualitative, and sequential scales
    with varying numbers of colors.  This file creates a data set
	&lib..brewer containing all 1683 combinations of category, palette
	and number of colors from 3..maxc, where maxc is the maximum number of
	colors in a given category-palette combination.

    See: http://www.personal.psu.edu/faculty/c/a/cab38/ColorBrewerBeta2.html
	for an online application demonstrating these color scales.

    The data values used below were extracted from the RColorBrewer R package.
	In R, see them by

	install.package("RColorBrewer")
	library(RColorBrewer)
	display.brewer.all()

	# print out all brewer palettes
	get.brewer <- function() {
    	info <- brewer.pal.info
    	n <- dim(info)[1]
    	palettes <- rownames(info)
    	cat("category, palette, nc, maxc, colors, \n")
    	for (i in 1:n) {
        	pal <- palettes[i]
        	maxc <- info[i,1]
        	cat <- info[i,2]
        	for (j in 3:maxc) {
            	colors <- brewer.pal(j,pal)
            	cat (c(cat, pal, j, maxc, colors, "\n"))
        	}
    	}
	}

	For convenience, two sets of hsv diverging palettes are also included
	
*/

*-- For use in macros and other applications, we store the data set in
    a SAS library;
libname colors '/covid_analysis/SAS_V2';
%let lib = colors;
*-- Use this to disable creation of a permanent data set;
%*let lib = work;  

* Use options fmtlib=(&lib) to access these formats in a later run;
* As used here, the variables CATEGORY and PALDESC are created using
  the formats CAT. and $PALETTE., so it is probably not necessary to
  store the formats to a permanent format library.;
    
proc format
	/* lib = &lib */
	;
	*-- Formats for translating category numbers into character category types;
    value cat 1='div' 2='qual' 3='seq';
    value category 1='diverging' 2='qualitative' 3='sequential';

	*-- Formats for translating palette abbreviations to descriptive labels;
	value $palette
		/* diverging colors */
		'BrBG' = 'Brown-Blue-Green'
		'PiYG' = 'Magenta-Yellow-Green'
		'PRGn' = 'Purple-Red-Green'
		'PuOr' = 'Purple-Orange'
		'RdBu' = 'Red-Blue'
		'RdGy' = 'Red-Grey'
		'RdYlBu'='Red-Yellow-Blue'
		'RdYlGn'='Red-Yellow-Green'
		'Spectral'='Spectral colors'
		/* qualitative colors */
		'Accent' = 'Accents'
		'Dark2'  = 'Dark colors'
		'Paired' = 'Paired colors'
		/* sequential colors */
		'Blues' = 'Blue shades'
		'BuGn'  = 'Blue-Green shades'
		'BuPu'  = 'Blue-Purple shades'
		'GnBu'  = 'Green-Blue shades'
		'Greens'= 'Green shades'
		'Greys' = 'Grey shades'
		'Oranges'='Orange shades'
		'OrRd'  = 'Orange-Red shades'
		'PuBu'  = 'Purple-Blue shades'
		'PuBuGn'= 'Purple-Blue-Green shades'
		'PuRd'  = 'Purple-Red shades'
		'Purples'='Purple shades'
		'RdPu'  = 'Red-Purple shades'
		'Reds'  = 'Red shades'
		'YlGn'  = 'Yellow-Green shades'
		'YlGnBu'= 'Yellow-Green-Blue shades'
		'YlOrBr'= 'Yellow-Orange-Brown shades'
		'YlOrRd'= 'Yellow-Orange-Red shades'
		/* HSV diverging colors */
		'hsv-RdBu' = 'HSV Red-Blue'
		'hsv-CyMg' = 'HSV Cyan-Magenta'
		;
data &lib..brewer;
    input cat palette $ nc maxc @;
    category = put(cat, cat.);
	paldesc = put(palette, $palette.);
    do colnum=1 to nc;
        input color $ @;
        color = 'CX' || substr(color,2);
		*-- Extract R, G, B (decimal) components, 0..255;
		red = input(substr(color,3,2),hex.);
		green = input(substr(color,5,2),hex.);
		blue = input(substr(color,7,2),hex.);
        output;
        end;
	label
		cat      ='Palette category'
		category ='Palette type'
		palette = 'Palette name'
		paldesc = 'Palette description'
		colnum  = 'Color number'
		nc = 'Number of colors'
		maxc = 'Maximum number of colors'
		color = 'RGB color (hex)'
		red = 'Red (dec)'
		green = 'Green (dec)'
		blue = 'Blue (dec)'
	;
*category, palette, nc, maxc, colors; 
datalines;
